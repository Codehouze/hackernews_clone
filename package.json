{
  "name": "hackernews_clone",
  "version": "1.0.0",
  "description": "# Description ## Hackernews is a platform where people can share links to websites and people can comment on them.",
  "main": "index.js",
  "husky":{
    "hooks":{
      "pre-commit": "prettier --write . && git add -A ."
    }
